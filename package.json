{
  "name": "basic_config",
  "version": "0.0.1",
  "description": "basic vanilla js dev env",
  "main": "index.js",
  "scripts": {
    "test": "jest .",
    "type-check": "tsc --noEmit",
    "type-check:watch": "npm run type-check -- --watch",
